    "def removeDuplicates(nums):\n",
    "    \"\"\"\n",
    "    :type nums: List[int]\n",
    "    :rtype: int\n",
    "    \"\"\"\n",
    "    i = 0\n",
    "    for n in nums:\n",
    "        if i < 2 or n != nums[i-2]:\n",
    "            nums[i] = n\n",
    "            i += 1\n",
    "    return i"
   ]
